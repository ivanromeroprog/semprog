<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.seminarioprogramacion.controlador.VehiculosListadoController">
   <children>
      <VBox alignment="CENTER" prefHeight="299.0" prefWidth="842.0" spacing="20.0">
         <children>
            <TableView fx:id="lista" prefHeight="179.0" prefWidth="401.0">
              <columns>
                <TableColumn prefWidth="113.0" text="Patente" />
                  <TableColumn prefWidth="164.0" text="Marca" />
                  <TableColumn prefWidth="157.0" text="Modelo" />
                  <TableColumn prefWidth="114.0" text="Aseguradora" />
                  <TableColumn prefWidth="105.0" text="Nº Póliza" />
                  <TableColumn prefWidth="148.0" text="Titular" />
              </columns>
            </TableView>
            <FlowPane alignment="CENTER" prefHeight="26.0" prefWidth="284.0">
               <children>
                  <Button fx:id="btningreso1" onAction="#switchToVehiculos" text="Nuevo">
                     <FlowPane.margin>
                        <Insets left="15.0" right="15.0" />
                     </FlowPane.margin></Button>
                  <Button fx:id="btningreso2" onAction="#switchToVehiculosEditar" text="Editar">
                     <FlowPane.margin>
                        <Insets left="15.0" right="15.0" />
                     </FlowPane.margin></Button>
                  <Button fx:id="btningreso" onAction="#eliminar" text="Eliminar" textOverrun="WORD_ELLIPSIS">
                     <FlowPane.margin>
                        <Insets left="15.0" right="15.0" />
                     </FlowPane.margin></Button>
               </children>
            </FlowPane>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</VBox>
